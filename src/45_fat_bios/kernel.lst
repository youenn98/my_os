   306 00000CD2 D9FE                <1> 		fsin									;  sin(kθ)|         |         |         |         |         |
   307 00000CD4 D8CB                <1> 		fmul	st0, st3						; Asin(kθ)|         |         |         |         |         |
   308                              <1> 												; ---------+---------+---------|---------|---------|---------|
   309                              <1> 												; Asin(kθ)|  sin(θ)|  cos(θ)|       A |       k |       r |
   310                              <1> 												; ---------+---------+---------|---------|---------|---------|
   311                              <1> 		;---------------------------------------
   312                              <1> 		; x =  A * sin(kθ) * cos(θ);
   313                              <1> 		;---------------------------------------
   314                              <1> 												; ---------+---------+---------|---------|---------|---------|
   315                              <1> 												;       ST0|      ST1|      ST2|      ST3|      ST4|      ST5|
   316                              <1> 												; ---------+---------+---------|---------|---------|---------|
   317                              <1> 												; Asin(kθ)|  sin(θ)|  cos(θ)|       A |       k |       r |
   318 00000CD6 D9CA                <1> 		fxch	st2								;   cos(θ)|         |Asin(kθ)|         |         |         |
   319 00000CD8 D8CA                <1> 		fmul	st0, st2						;        x |         |         |         |         |         |
   320 00000CDA DB18                <1> 		fistp	dword [eax]						;   sin(θ)|Asin(kθ)|       A |       k |       r |xxxxxxxxx|
   321                              <1> 												; ---------+---------+---------|---------|---------|---------|
   322                              <1> 		;---------------------------------------
   323                              <1> 		; y = -A * sin(kθ) * sin(θ);
   324                              <1> 		;---------------------------------------
   325                              <1> 												; ---------+---------+---------|---------|---------|---------|
   326                              <1> 												;       ST0|      ST1|      ST2|      ST3|      ST4|      ST5|
   327                              <1> 												; ---------+---------+---------|---------|---------|---------|
   328                              <1> 												;   sin(θ)|Asin(kθ)|       A |       k |       r |xxxxxxxxx|
   329 00000CDC DEC9                <1> 		fmulp	st1, st0						;        y |       A |       k |       r |xxxxxxxxx|xxxxxxxxx|
   330 00000CDE D9E0                <1> 		fchs									;       -y |         |         |         |xxxxxxxxx|xxxxxxxxx|
   331 00000CE0 DB1B                <1> 		fistp	dword [ebx]						;        A |       k |       r |xxxxxxxxx|xxxxxxxxx|xxxxxxxxx|
   332                              <1> 												; ---------+---------+---------|---------|---------|---------|
   333 00000CE2 5B                  <1> 		pop		ebx
   334 00000CE3 58                  <1> 		pop		eax
